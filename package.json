{
  "name": "var-translation",
  "displayName": "vscode变量翻译命名工具",
  "description": "使用Google Translate API翻译语言到英文并且转换成常见的编码格式",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "translate",
    "google translate",
    "翻译"
  ],
  "activationEvents": [
    "onCommand:extension.varTranslation"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.varTranslation",
        "key": "meta+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "extension.varTranslation",
        "title": "varTranslation",
        "when": "editorTextFocus",
        "category": "var-translation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "translation.js": "^0.7.8"
  }
}