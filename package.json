{
  "name": "var-translation",
  "displayName": "var-translation",
  "description": "使用翻译引擎快速翻译转换变量到常用的英文命名格式(大小驼峰等)",
  "publisher": "svenzhao",
  "icon": "images/logo.png",
  "version": "0.2.2",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "translate",
    "google translate",
    "翻译"
  ],
  "activationEvents": [
    "onCommand:extension.varTranslation"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.varTranslation",
        "key": "Alt+shift+t",
        "mac": "cmd+shift+t"
      }
    ],
    "commands": [
      {
        "command": "extension.varTranslation",
        "title": "varTranslation",
        "when": "editorTextFocus",
        "category": "var-translation"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "extension.varTranslation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "varTranslation configuration",
      "properties": {
        "varTranslation.translationEngine": {
          "type": "string",
          "enum": [
            "google",
            "youdu",
            "baidu"
          ],
          "enumDescriptions": [
            "谷歌",
            "有道",
            "百度"
          ],
          "default": "'google'",
          "description": "翻译引擎"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run release && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "release": "standard-version",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "standard-version": "^6.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "translation.js": "^0.7.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SvenZhao/var-translation"
  },
  "homepage": "https://github.com/SvenZhao/var-translation/blob/master/README.md"
}
